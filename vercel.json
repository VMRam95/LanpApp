{
  "version": 2,
  "buildCommand": "pnpm --filter @lanpapp/shared run build && pnpm run build:vercel && pnpm run build",
  "installCommand": "pnpm install",
  "outputDirectory": "apps/web/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/((?!api|assets|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}
